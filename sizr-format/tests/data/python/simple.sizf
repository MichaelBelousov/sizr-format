
node "module" = $CHILDREN

node "def" = "def "

node "(" = "("
node "," = ","
node ")" = ")"
node ":" = ":\n"

node "block" = { |> $CHILDREN "\n" <| }

node "function_definition" = {
	"def" $name "(" $parameters "):\n"
		$body
}

node "if" = "if"
node "else" = "else"

node "if_statement" = {
	$condition ":\n" $consequence "else:\n" $alternative
}

node "else_clause" = $body

node "argument_list" = { "(" $NAMED_CHILDREN ")" }

node "parameters" = $CHILDREN

node "identifier" = "BLAH"
