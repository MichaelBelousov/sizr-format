{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    /*
    {
      "type": "lldb-mi",
      "request": "launch",
      "name": "(lldb-mi) launch",
      "target": "/home/mike/personal/zig/zig-out/bin/zig",
      "arguments": "test ${workspaceRoot}/src/code.zig",
      "cwd": "${workspaceRoot}",
      "valuesFormatting": "parseText"
    },
    {
      "type": "lldb-vscode",
      "request": "launch",
      "name": "(lldb-vscode) launch",
      "program": "/home/mike/personal/zig/zig-out/bin/zig",
      "args": ["test", "${workspaceRoot}/src/code.zig"],
      "env": [],
      "cwd": "${workspaceRoot}"
    },
    */
    {
      "name": "(gdb) launch",
      "type": "gdb",
      "request": "launch",
      "target": "/home/mike/personal/zig/build/bin/zig",
      "arguments": "build-exe --strip -O ReleaseSmall src/code.zig",
      "cwd": "${workspaceRoot}",
      //"valuesFormatting": "parseText"
      //"valuesFormatting": "prettyPrinters",
      "valuesFormatting": "parseText",
      "autorun": [
        "set print pretty",
        "set print union off"
      ]
    },
    {
      "name": "(gdb) launch *control*",
      "type": "gdb",
      "request": "launch",
      "target": "/home/mike/personal/zig/build/bin/zig",
      "arguments": "build-exe --strip -O ReleaseSmall src/code2.zig --cache-dir zig-cache2",
      "cwd": "${workspaceRoot}",
      //"valuesFormatting": "parseText"
      //"valuesFormatting": "prettyPrinters",
      "valuesFormatting": "parseText",
      "preLaunchTask": "remove-zig-cache",
      "autorun": [
        "pause",
        "set print pretty",
        "set print union off"
      ]
    }
  ]
}
